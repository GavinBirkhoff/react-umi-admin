    "react-dom": "^18.2.0",
    "storetify": "^1.0.0",
    "swiftlet-log": "1.0.0-beta.1",
    "ts-copilot": "1.0.0-Beta",
    "umi": "^4.1.1",
    "use-compare": "^1.0.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@umijs/lint": "^4.1.1",
    "@umijs/plugins": "^4.1.1",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "express": "^4.18.2",
    "lint-staged": "^15.2.2",
    "prettier": "^2.8.8",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.3",
